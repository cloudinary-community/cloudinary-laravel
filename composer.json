{
  "name": "cloudinary-labs/cloudinary-laravel",
  "description": "A Laravel Cloudinary Package",
  "license": "MIT",
  "authors": [
    {
      "name": "Josh Manders",
      "homepage": "https://github.com/joshmanders"
    }
  ],
  "homepage": "https://github.com/cloudinary-labs/cloudinary-laravel",
  "keywords": [
    "Laravel",
    "cloudinary-laravel",
    "File Uploads",
    "Media Management",
    "Cloudinary",
    "File Transformations"
  ],
  "require": {
    "php": "^8.2",
    "ext-json": "*",
    "illuminate/support": "^11.0|^12.0",
    "cloudinary/cloudinary_php": "^3.0"
  },
  "require-dev": {
    "mockery/mockery": "^1.6",
    "orchestra/testbench": "^8.0|^9.0|^10.0",
    "pestphp/pest": "^2.35|^3.0",
    "sempro/phpunit-pretty-print": "^1.2",
    "laravel/pint": "^1.20",
    "phpspec/prophecy-phpunit": "^2.3"
  },
  "autoload": {
    "psr-4": {
      "CloudinaryLabs\\CloudinaryLaravel\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "CloudinaryLabs\\CloudinaryLaravel\\CloudinaryServiceProvider"
      ]
    }
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
